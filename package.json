{
  "name": "aottg-custom-logic",
  "displayName": "Aottg Custom Logic",
  "description": "Syntax highlighting for Aottg CustomLogic",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "experiment.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "acl",
        "aliases": [
          "CustomLogic",
          "acl"
        ],
        "extensions": [
          ".acl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "acl",
        "scopeName": "source.acl",
        "path": "./syntaxes/acl.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "acl",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikr2202/acl-vscode"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
    "@types/vscode": "1.78.0",
    "@types/node": "16.x",
    "typescript": "^5.0.4"
  }
}
